BYTECCCOMPOPTS=-fno-defer-pop -Wall -D_FILE_OFFSET_BITS=64 ANDROID_CFLAGS
BYTECCLINKOPTS=-Wl,-E ANDROID_LDFLAGS
BYTECCLIBS=ANDROID_MATHLIB -ldl
BYTECCRPATH=-Wl,-rpath,
